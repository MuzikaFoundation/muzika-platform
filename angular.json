{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "muzika-core": {
      "root": "projects/core",
      "projectType": "library",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "project": "projects/core/ng-package.json",
            "tsConfig": "projects/core/tsconfig.json"
          },
          "configurations": {
            "production": {
              "project": "projects/core/ng-package.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/core/src/test.ts",
            "tsConfig": "projects/core/tsconfig.spec.json",
            "karmaConfig": "projects/core/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/core/tsconfig.json",
              "projects/core/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "muzika-ipfs-viewer": {
      "root": "",
      "sourceRoot": "projects/viewer/src/",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/muzika-viewer",
            "index": "projects/viewer/src/index.html",
            "main": "projects/viewer/src/main.ts",
            "tsConfig": "projects/viewer/tsconfig.json",
            "polyfills": "projects/viewer/src/polyfills.ts",
            "assets": [
              "projects/viewer/src/assets",
              "projects/viewer/src/favicon.ico",
              "projects/viewer/src/favicon.png",
              "projects/viewer/src/favicon.icns",
              "projects/viewer/src/favicon.256x256.png",
              "projects/viewer/src/favicon.512x512.png",
              "projects/viewer/src/angular-scripts.js"
            ],
            "styles": [
              "projects/viewer/src/styles.scss",
              "scss/fontawesome/css/fontawesome.min.css",
              "scss/fontawesome/css/fontawesome-all.min.css",
              "node_modules/froala-editor/css/froala_editor.min.css",
              "node_modules/froala-editor/css/froala_editor.pkgd.min.css",
              "node_modules/froala-editor/css/froala_style.min.css",
              "node_modules/froala-editor/css/plugins/video.min.css",
              "node_modules/froala-editor/css/plugins/colors.min.css",
              "node_modules/froala-editor/css/plugins/table.min.css",
              "node_modules/froala-editor/css/plugins/emoticons.min.css",
              "projects/viewer/src/theme/froala-theme.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": true,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "projects/viewer/src/environments/environment.ts",
                  "with": "projects/viewer/src/environments/environment.prod.ts"
                }
              ]
            },
            "stage": {
              "optimization": false,
              "outputHashing": "none",
              "sourceMap": true,
              "extractCss": true,
              "namedChunks": false,
              "aot": false,
              "extractLicenses": false,
              "vendorChunk": false,
              "buildOptimizer": false,
              "fileReplacements": [
                {
                  "replace": "projects/viewer/src/environments/environment.ts",
                  "with": "projects/viewer/src/environments/environment.stage.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "muzika-ipfs-viewer:build",
            "hmr": true,
            "hmrWarning": false
          },
          "configurations": {
            "production": {
              "browserTarget": "muzika-ipfs-viewer:build:production"
            },
            "stage": {
              "browserTarget": "muzika-ipfs-viewer:build:stage"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "muzika-ipfs-viewer:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/viewer/src/test.ts",
            "polyfills": "projects/viewer/src/polyfills-test.ts",
            "tsConfig": "projects/viewer/tsconfig.spec.json",
            "karmaConfig": "projects/viewer/src/karma.conf.js",
            "scripts": [],
            "styles": [
              "projects/viewer/src/styles.scss",
              "scss/fontawesome/css/fontawesome.min.css",
              "scss/fontawesome/css/fontawesome-all.min.css"
            ],
            "assets": [
              "projects/viewer/src/assets",
              "projects/viewer/src/favicon.ico",
              "projects/viewer/src/favicon.png",
              "projects/viewer/src/favicon.icns",
              "projects/viewer/src/favicon.256x256.png",
              "projects/viewer/src/favicon.512x512.png"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/viewer/tsconfig.json",
              "projects/viewer/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "muzika-ipfs-viewer-e2e": {
      "root": "e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "muzika-ipfs-viewer:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "muzika-web": {
      "root": "projects/web/",
      "sourceRoot": "projects/web/",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/web",
            "index": "projects/web/src/index.html",
            "main": "projects/web/src/main.ts",
            "tsConfig": "projects/web/tsconfig.json",
            "polyfills": "projects/web/src/polyfills.ts",
            "assets": [
              {
                "glob": "favicon.ico",
                "input": "projects/web/src/",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "projects/web/src/assets",
                "output": "/assets"
              },
              {
                "glob": "particles.js",
                "input": "node_modules/particles.js/",
                "output": "/assets"
              }
            ],
            "styles": [
              "projects/web/src/styles.scss",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "scss/fontawesome/css/fontawesome.min.css",
              "scss/fontawesome/css/fontawesome-all.min.css"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.min.js",
              "node_modules/froala-editor/js/froala_editor.min.js",
              "node_modules/froala-editor/js/languages/ko.js",
              "node_modules/froala-editor/js/plugins/link.min.js",
              "node_modules/froala-editor/js/plugins/table.min.js",
              "node_modules/froala-editor/js/plugins/url.min.js",
              "node_modules/froala-editor/js/plugins/colors.min.js",
              "node_modules/froala-editor/js/plugins/align.min.js",
              "node_modules/froala-editor/js/plugins/emoticons.min.js",
              "node_modules/froala-editor/js/plugins/font_size.min.js",
              "node_modules/froala-editor/js/plugins/code_view.min.js",
              "node_modules/froala-editor/js/plugins/lists.min.js",
              "node_modules/froala-editor/js/plugins/paragraph_style.min.js",
              "node_modules/froala-editor/js/plugins/image.min.js",
              "node_modules/froala-editor/js/plugins/video.min.js",
              "node_modules/froala-editor/js/plugins/image_manager.min.js"
            ]
          },
          "configurations": {
            "stage": {
              "fileReplacements": [
                {
                  "replace": "projects/web/src/environments/environment.ts",
                  "with": "projects/web/src/environments/environment.stage.ts"
                }
              ]
            },
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "projects/web/src/environments/environment.ts",
                  "with": "projects/web/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "muzika-web:build",
            "hmr": true,
            "hmrWarning": false
          },
          "configurations": {
            "stage": {
              "browserTarget": "muzika-web:build:stage"
            },
            "production": {
              "browserTarget": "muzika-web:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "muzika-web:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/web/src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "projects/web/src/polyfills.ts",
            "tsConfig": "projects/web/tsconfig.spec.json",
            "scripts": [],
            "styles": [
              "projects/web/src/styles.scss",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "scss/fontawesome/css/fontawesome.min.css",
              "scss/fontawesome/css/fontawesome-all.min.css"
            ],
            "assets": [
              {
                "glob": "favicon.ico",
                "input": "projects/web/src/",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "projects/web/src/assets",
                "output": "/assets"
              }
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/core/tsconfig.json",
              "projects/web/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "muzika-web-e2e": {
      "root": "",
      "sourceRoot": "",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "muzika-web:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "app": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "progress": false,
            "outputPath": "www",
            "index": "projects/ionic-app/index.html",
            "main": "projects/ionic-app/main.ts",
            "polyfills": "projects/ionic-app/polyfills.ts",
            "tsConfig": "projects/ionic-app/tsconfig.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/ionic-app/assets",
                "output": "assets"
              },
              {
                "glob": "**/*.svg",
                "input": "node_modules/@ionic/angular/dist/ionic/svg",
                "output": "./svg"
              },
              {
                "glob": "**/*",
                "input": "platforms/android/platform_www",
                "output": "./"
              }
            ],
            "styles": [
              {
                "input": "projects/ionic-app/theme/variables.scss"
              },
              {
                "input": "projects/ionic-app/global.scss"
              },
              "scss/fontawesome/css/fontawesome.min.css",
              "scss/fontawesome/css/fontawesome-all.min.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/ionic-app/environments/environment.ts",
                  "with": "projects/ionic-app/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "app:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "app:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ionic-app/test.ts",
            "polyfills": "projects/ionic-app/polyfills.ts",
            "tsConfig": "projects/ionic-app/tsconfig.spec.json",
            "karmaConfig": "projects/ionic-app/karma.conf.js",
            "styles": [
              "styles.css"
            ],
            "scripts": [],
            "assets": [
              {
                "glob": "favicon.ico",
                "input": "projects/ionic-app/",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "projects/ionic-app/assets",
                "output": "/assets"
              }
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ionic-app/tsconfig.json",
              "projects/ionic-app/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "app",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "app",
      "styleext": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "app"
    }
  }
}
