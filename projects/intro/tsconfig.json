{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "../../out-tsc/app",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "module": "commonjs",
    "types": [
      "node",
      "web3"
    ],
    "typeRoots": [
      "./src/typings.d.ts",
      "../../node_modules/@types",
      "../../node_modules/@0xproject/typescript-typings/types"
    ],
    "lib": [
      "es2016",
      "es2015",
      "dom"
    ],
    "paths": {
    }
  },
  "exclude": [
    "src/test.ts",
    "**/*.spec.ts",
    "dist",
    "app-builds",
    "node_modules"
  ]
}
