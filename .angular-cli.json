{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "muzika-core"
  },
  "apps": [
    {
      "root": "projects/wallet/src",
      "outDir": "dist/wallet",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "../tsconfig.json",
      "testTsconfig": "../tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.scss",
        "../../../node_modules/bootstrap/scss/bootstrap.scss",
        "../../../scss/fontawesome-pro/css/fontawesome.min.css",
        "../../../scss/fontawesome-pro/css/fontawesome-all.min.css"
      ],
      "scripts": [],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "stage": "environments/environment.stage.ts",
        "prod": "environments/environment.prod.ts"
      }
    },
    {
      "root": "projects/prototype/src",
      "outDir": "dist/prototype",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "../tsconfig.json",
      "testTsconfig": "../tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.scss",
        "../../../node_modules/bootstrap/scss/bootstrap.scss",
        "../../../scss/fontawesome-pro/css/fontawesome.min.css",
        "../../../scss/fontawesome-pro/css/fontawesome-all.min.css"
      ],
      "scripts": [],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "stage": "environments/environment.stage.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "projects/core/tsconfig.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "projects/core/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "projects/wallet/tsconfig.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "projects/wallet/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "projects/prototype/tsconfig.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "projects/prototype/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "component": {}
  }
}
